<h1>Editing media</h1>

<% form_for(@media) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :artifact_id %><br />
    <%= f.text_field :artifact_id %>
  </p>
  <p>
    <%= f.label :media_name %><br />
    <%= f.text_field :media_name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @media %> |
<%= link_to 'Back', medias_path %>